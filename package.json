{
  "name": "@aokiapp/jsapdu-over-ip",
  "version": "0.0.1",
  "description": "jsapdu インターフェースをHTTP経由でミラーリング",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "turbo run build lint typecheck && changeset publish",
    "test:ci": "SKIP_E2E_TESTS=true vitest run",
    "test": "vitest run"
  },
  "keywords": [
    "jsapdu",
    "smartcard",
    "remote",
    "http",
    "rpc"
  ],
  "author": "AokiApp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AokiApp/jsapdu-over-ip.git"
  },
  "devDependencies": {
    "@aokiapp/jsapdu-interface": "^0.0.3",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.10",
    "turbo": "^1.13.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "@aokiapp/jsapdu-interface": "^0.0.3"
  }
}
