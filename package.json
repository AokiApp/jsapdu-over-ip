    "type": "git",
    "url": "https://github.com/AokiApp/jsapdu-over-ip.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "@aokiapp/jsapdu-interface": "^0.0.2"
  },
  "peerDependencies": {
    "@aokiapp/jsapdu-interface": "^0.0.2"
  }
}